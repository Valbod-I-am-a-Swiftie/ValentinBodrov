<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test All">

    <test name="Sum Test">
        <classes>
            <class name="hw1.CalculatorSumTest">
                <methods>
                    <include name="sumTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Substract Test">
        <classes>
            <class name="hw1.CalculatorSubTest">
                <methods>
