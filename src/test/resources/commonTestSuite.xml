                </methods>
            </class>
        </classes>
    </test>

    <test name="Multiply Test">
        <classes>
            <class name="hw1.CalculatorMultTest">
                <methods>
                    <include name="multTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Divide Test">
        <classes>
            <class name="hw1.CalculatorDivTest">
                <methods>
                    <include name="divTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Sqrt Test">
        <classes>
            <class name="hw1.CalculatorSqrtTest">
                <methods>
                    <include name="sqrtTest"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>